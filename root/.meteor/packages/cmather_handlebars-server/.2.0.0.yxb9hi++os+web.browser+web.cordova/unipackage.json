{
  "name": "cmather:handlebars-server",
  "summary": "Allows handlebars templates to be defined on the server in .handlebars files",
  "version": "2.0.0",
  "isTest": false,
  "plugins": [
    {
      "name": "compileServerHandlebarsTemplates",
      "arch": "os",
      "path": "plugin.compileServerHandlebarsTemplates.os/program.json"
    }
  ],
  "unibuilds": [
    {
      "kind": "main",
      "arch": "os",
      "path": "os-legacy.json"
    },
    {
      "kind": "main",
      "arch": "web.browser",
      "path": "web.browser-legacy.json"
    },
    {
      "kind": "main",
      "arch": "web.cordova",
      "path": "web.cordova-legacy.json"
    }
  ],
  "format": "unipackage-pre2"
}